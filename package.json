{
  "name": "wallet-backend",
  "version": "1.0.0",
  "description": "Serverless using Typescript",
  "scripts": {
    "start": "yarn run sls offline --stage dev",
    "start:staging": "yarn run sls offline --stage stage",
    "start:production": "yarn run sls offline --stage prod",
    "start:debug": "SLS_DEBUG=* && node --inspect=9229 ./node_modules/.bin/serverless offline --stage dev",
    "deploy": "yarn run sls deploy --stage dev",
    "deploy:staging": "yarn run sls deploy --stage stage",
    "deploy:production": "yarn run sls deploy --stage prod"
  },
  "dependencies": {
    "bluebird": "^3.5.5",
    "@types/aws-serverless-express": "^3.3.1",
    "@types/body-parser": "latest",
    "@types/express": "latest",
    "path-to-regexp": "^3.0.0",
    "serverless": "^1.32.0",
    "serverless-offline": "^5.8.0",
    "serverless-plugin-typescript": "^1.1.3",
    "source-map-support": "^0.5.10"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/node": "^10.12.18",
    "serverless-webpack": "^5.2.0",
    "ts-loader": "^5.3.3",
    "tslint": "^5.7.0",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
